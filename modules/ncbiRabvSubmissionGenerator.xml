<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<gbSubmisisonGenerator>

	<assemblyGapSpecifier>
		<lengthsKnown>true</lengthsKnown>
		<minRunLengthForGap>50</minRunLengthForGap>
		<linkageEvidence>align_genus</linkageEvidence>
	</assemblyGapSpecifier>
		
	<sourceInfoProviders>
	
		<staticSourceInfoProvider>
			<sourceModifier>organism</sourceModifier>
			<value>Rabies virus</value>
		</staticSourceInfoProvider>
		
		<staticSourceInfoProvider>
			<sourceModifier>moltype</sourceModifier>
			<value>genomic</value>
		</staticSourceInfoProvider>
		
		<staticSourceInfoProvider>
			<sourceModifier>molecule</sourceModifier>
			<value>rna</value>
		</staticSourceInfoProvider>
		
		<freemarkerSourceInfoProvider>
			<sourceModifier>host</sourceModifier>
			<template>${host!""}</template>
		</freemarkerSourceInfoProvider>
			
		<freemarkerSourceInfoProvider>
			<sourceModifier>isolate</sourceModifier>
			<template>${isolate!""}</template>
		</freemarkerSourceInfoProvider>
		
		<freemarkerSourceInfoProvider>
			<sourceModifier>collection-date</sourceModifier>
			<template>${collection_year!""}</template>
		</freemarkerSourceInfoProvider>

		<seqPropertySourceInfoProvider>
			<sourceModifier>country</sourceModifier>
			<propertyPath>m49_country.display_name</propertyPath>
		</seqPropertySourceInfoProvider>

		
	</sourceInfoProviders>
	
